<uwsgi>
    <!--// <socket>:8077</socket>//桥接端口,nginx 也要设置为此端口 -->
    <socket>:8006</socket><!-- unix socket file,crate a empty file eg:/root/ENV35/bin/my_blog/my_blog/my_blog.sock -->
    <chmod-socket>664</chmod-socket> <!-- permission to access socket file-->
    <chdir>/root/ENV35/bin/my_blog/my_blog</chdir>
    <module>wsgi</module> <!--config file is wsgi.py -->
    <processes>4</processes> <!-- 进程数 --> 
    <daemonize>uwsgi.log</daemonize>
    <protocol>http</protocol>
</uwsgi>
